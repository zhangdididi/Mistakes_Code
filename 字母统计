import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String str = in.next();
        int[] chars = new int[26];
        for (char c : str.toCharArray()) {
            if (c >= 'A' && c <= 'Z') {
                chars[c - 'A']++;
            }
        }
        for (int i = 0; i < 26; i++) {
            System.out.println((char)(i + 'A') + ":" + chars[i]);
        }
    }
}
